{
    "manifest_version": 2,

    "name": "abctx",
    "description": "This extension shows widgets that aware browsing contexts.",
    "version": "0.0.1",
    
    "permissions": [
        "storage",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "js": [ "jquery-2.1.0.min.js", "jquery-ui-1.10.4.custom.min.js", "popup.js" ],
        "css": ["jquery-ui-1.10.4.custom.min.css"],
        "matches": [ "http://*/*", "https://*/*"]
    }]
}
